{
    "$schema": "https://turbo.build/schema.json",
    "globalDependencies": [".env", ".env.local", "tsconfig.json"],
    "globalDotEnv": [".env"],
    "pipeline": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": [".next/**", "!.next/cache/**"]
        },
        "lint": {},
        "dev": {
            "cache": false,
            "persistent": true
        },
        "start": {
            "cache": true,
            "persistent": true,
            "dotEnv": [".env.production"]
        }
    }
}
